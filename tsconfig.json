{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "baseUrl": ".",
    "paths": {
      "prismjs": ["node_modules/prismjs"],
      "prismjs/*": ["node_modules/prismjs/*"]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "outDir": "./dist/",
    "strict": true,
    "allowJs": true
  }
}
